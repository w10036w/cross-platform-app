{
  "name": "crossapp",
  "version": "1.0.0",
  "keywords": [],
  "description": "Cross Platform (React Native & React Native Web)",
  "scripts": {
    "eject": "react-native eject",
    "start": "cross-env NODE_ENV=development react-native start",
    "start:reset": "NODE_ENV=development react-native start --reset-cache",
    "start:web": "cross-env PLATFORM=web node scripts/start.js",
    "start:stub": "PLATFORM=web DATA_ENV=static npm run start",
    "ios": "react-native run-ios --simulator=\"iPhone 8\"",
    "ios:tablet": "react-native run-ios --simulator=\"iPad Air 2\"",
    "android": "react-native run-android",
    "build": "react-native bundle",
    "build:web": "PLATFORM=web node scripts/build.js",
    "test:web": "PLATFORM=web node scripts/test.js --env=jsdom",
    "schema": "graphql get-schema --project gateway --output ./src/schema.graphql",
    "voyager": "graphql voyager",
    "web": "npm run start:web"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@ant-design/react-native": "^3.1.8",
    "@emotion/core": "^10.0.10",
    "@emotion/native": "^10.0.11",
    "@emotion/primitives": "^10.0.10",
    "axios": "^0.18.0",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "history": "^4.9.0",
    "lodash": "^4.17.11",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-intl": "^2.9.0",
    "react-loadable": "^5.5.0",
    "react-native": "0.59.9",
    "react-native-svg": "9.4.0",
    "react-native-svg-uri": "^1.2.3",
    "react-native-web": "0.11.2",
    "react-primitives": "^0.8.0",
    "react-router-cache-route": "^1.4.2",
    "react-router-dom": "^5.0.0",
    "react-router-native": "^5.0.0",
    "react-spring": "^8.0.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/parser": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.4",
    "@svgr/webpack": "4.2.0",
    "@typescript-eslint/eslint-plugin": "1.8.0",
    "@typescript-eslint/parser": "1.8.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-emotion": "^10.0.9",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-macros": "^2.5.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-preset-minify": "^0.5.0",
    "babel-preset-react-app": "^9.0.0",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "css-loader": "2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^4.0.1",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-emotion": "^10.0.7",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "jest": "24.7.1",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.7.1",
    "jest-watch-typeahead": "0.3.0",
    "metro-react-native-babel-preset": "^0.54.0",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "penv.macro": "^0.2.0",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.17.0",
    "prettier-eslint": "^8.8.2",
    "react-app-polyfill": "^1.0.1",
    "react-dev-utils": "^9.0.1",
    "react-test-renderer": "16.8.6",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "semver": "6.0.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.3",
    "ts-pnp": "1.1.2",
    "url-loader": "1.1.2",
    "webpack": "4.31.0",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^react-native$": "react-native-web"
    },
    "modulePaths": [],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js",
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
