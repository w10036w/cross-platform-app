    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "graphql": "^14.3.0",
    "history": "^4.9.0",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
