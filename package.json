{
  "name": "crossapp",
  "version": "1.0.0",
  "description": "React Native for Web starter template",
  "keywords": [],
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "native": "NODE_ENV=development react-native start",
    "native:reset": "NODE_ENV=development react-native start --reset-cache",
    "start:env": "cp -v config/babel.env.js babel.config.js & npm run native:reset",
    "start:stub": "DATA_ENV=static NODE_ENV=development npm run start",
    "ios": "react-native run-ios --simulator=\"iPhone 8\"",
    "ios:tablet": "react-native run-ios --simulator=\"iPad Air 2\"",
    "android": "react-native run-android"
  },
